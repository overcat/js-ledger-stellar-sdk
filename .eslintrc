{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier", "jest", "jsdoc"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:jsdoc/recommended"
  ],
  "rules": {
    "no-console": 1,
    "prettier/prettier": 2
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
